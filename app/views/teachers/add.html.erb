<% provide(:title, "CSV Hochladen") %>
<div class="center jumbotron">
  <h4>Import Teacher Data</h4>
    <%= form_tag import_teachers_path, multipart: true do %>
      <%= file_field_tag :file %>
      <%= submit_tag "Import CSV", data: { disable_with: "Wird hochgeladen..." }, class: "btn btn-primary" %>
    <% end  %>
</div>
